{
  "builds": [
    {
      "src": "api/src/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.8",
        "buildCommand": "pip install -r api/requirements.vercel.txt && vercel-build-command"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/src/wsgi.py"
    }
  ]
}
